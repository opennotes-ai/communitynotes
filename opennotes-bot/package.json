{
  "name": "opennotes-bot",
  "version": "1.0.0",
  "description": "Discord bot for Community Notes functionality",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:dashboard",
    "build:dashboard": "vite build",
    "dev": "tsx watch src/index.ts",
    "dev:dashboard": "vite",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t opennotes-bot .",
    "docker:run": "docker-compose up",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "nats": "^2.29.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "postcss": "^8.5.6",
    "prisma": "^5.6.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.13",
    "twilio": "^4.19.0",
    "vite": "^7.1.7",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "discord",
    "bot",
    "community-notes",
    "moderation"
  ],
  "author": "",
  "license": "MIT",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}